<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IlaTopBar">
        <attr name="title" format="string" />
        <attr name="isTitleCentered" format="boolean" />
        <attr name="titleColor" format="color" />
        <attr name="leftIcon" format="reference" />
        <attr name="actionLeftIcon" format="reference" />
        <attr name="actionRightIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="IlaEditText">
        <attr name="lines" format="integer"/>
        <attr name="maxLines" format="integer"/>
        <attr name="minLines" format="integer"/>
        <attr name="hint" format="string" />
        <attr name="startIcon" format="reference" />
        <attr name="endIcon" format="reference" />
    </declare-styleable>
</resources>